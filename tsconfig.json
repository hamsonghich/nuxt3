{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "./*"
    ],
    "~/*": [
      "./*"
    ]
  },
  "allowJs": true,
  "lib": [
    "ESNext",
    "DOM"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "./**/*",
    "node_modules/nuxt/types.d.ts"
  ]
}
